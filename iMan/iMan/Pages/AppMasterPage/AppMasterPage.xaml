<?xml version="1.0" encoding="utf-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  x:Class="iMan.Pages.Views.AppMasterPage"
                  xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                  prism:ViewModelLocator.AutowireViewModel="True" Icon="hamburger.png">
     <MasterDetailPage.Master>
        <ContentPage Title="Menu" Icon="hamburger.png" >
            <StackLayout>
                <Button Image="ic_launcher.png" Text="Home" Command="{Binding NavigateCommand}" CommandParameter="NavigationPage/MainPage"/>
                <Button Image="addimage.png" Text="Settings" Command="{Binding NavigateCommand}" CommandParameter="NavigationPage/SettingsPage"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
</MasterDetailPage>
